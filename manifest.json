{
    "manifest_version": 2,
    "name": "No-override Launcher",
    "description": "Quick launch in no-override mode",
    "version": "1.0.0",
    "icons": {"128": "logo.png"},
    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html",
        "default_title": "No-override Launcher"
    },
    "permissions": [
        "http://*/*", "https://*/*", "tabs"
    ]
}